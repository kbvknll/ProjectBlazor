{
  "Jwt": {
    "Key": "your_secret_key_here", // Секретный ключ для подписи токена
    "Issuer": "https://localhost:7073", // Издатель токена
    "Audience": "https://localhost:7073", // Аудитория токена
    "ExpiryInMinutes": 60 // Время жизни токена
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
